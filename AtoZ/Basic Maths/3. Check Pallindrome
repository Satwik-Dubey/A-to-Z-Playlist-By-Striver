class Solution {
    public boolean isPalindrome(int x) {
        if(x<0){
            return false;
        }
         String sb = String.valueOf(x);
        int len=sb.length();
        for(int i=0;i<len;i++){
            if(sb.charAt(i)!=sb.charAt(len-i-1)){
                return false;
            }
        }
        return true;
    }

}
